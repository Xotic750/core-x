{
  "name": "core-x",
  "homepage": "http://xotic750.github.io/core-x/",
  "description": "Core bundle for project-x projects.",
  "version": "1.0.6",
  "keywords": [
    "javascript",
    "bundle",
    "core"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Xotic750/core-x.git"
  },
  "main": "core-x.js",
  "author": {
    "name": "Xotic750",
    "email": "Xotic750@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/Xotic750/core-x/issues"
  },
  "engines": {
    "node": "*"
  },
  "license": "MIT",
  "dependencies": {
    "es5-shim": "^4.3.1",
    "es6-shim": "^0.33.13",
    "json3": "^3.3.2",
    "cycle-x": "^1.0.1"
  },
  "devDependencies": {
    "jscs": "^2.5.1",
    "uglify-js": "^2.5.0",
    "uglifyify": "^3.0.1",
    "browserify": "12.0.1"
  },
  "scripts": {
    "style": "jscs core-x.js",
    "uglify": "npm run style && uglifyjs core-x.js -o core-x.min.js --compress --keep-fnames --mangle --comments --beautify ascii_only=true,beautify=false --source-map core-x.map",
    "build": "browserify -g [ uglifyify --no-mangle --beautify [ --ascii_only --no-beautify ] ] -e core-x.js -o lib/core-x.min.js"
  }
}
