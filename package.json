    "es6-shim": "^0.33.13",
    "json3": "^3.3.2"
  },
  "devDependencies": {
    "jscs": "^2.5.1",
    "uglifyify": "^3.0.1",
    "browserify": "12.0.1",
    "napa": "^2.0.2"
  },
  "browser": {
    "es5-shim": "./node_modules/es5-shim/es5-shim.min.js",
